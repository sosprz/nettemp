#! /bin/bash

dir=$( cd "$( dirname "$0" )" && cd ../../ && pwd )

sqlite3 $dir/dbf/nettemp.db "UPDATE sensors SET name='Taras' WHERE rom='0x280x880x9D0xF70x020x000x000xB6'"
sqlite3 $dir/dbf/nettemp.db "UPDATE sensors SET name='Sypialnia' WHERE rom='0x280x040x5E0x250x030x000x000xE0'"
sqlite3 $dir/dbf/nettemp.db "UPDATE sensors SET name='Kuchnia' WHERE rom='0x280x6C0x580x250x030x000x000x1E'"
sqlite3 $dir/dbf/nettemp.db "UPDATE sensors SET name='Dziewczyny' WHERE rom='0x280xDE0x6E0x250x030x000x000x6F'"
sqlite3 $dir/dbf/nettemp.db "UPDATE sensors SET name='Salon' WHERE rom='0x280x890x360x250x030x000x000x6F'"

